{
  "name": "bodiless-js",
  "description": "Bodiless-JS",
  "version": "0.0.34",
  "keywords": [
    "gatsby"
  ],
  "license": "Apache-2.0",
  "scripts": {
    "build:dev": "cross-env NODE_ENV=development lerna run build --stream",
    "build": "cross-env NODE_ENV=production lerna run build --stream && npm run build:css",
    "build:css": "node ./scripts/build-css",
    "build:packages": "cross-env NODE_ENV=production lerna run build --stream --ignore '@bodiless/test-site'",
    "build:watch": "cross-env NODE_ENV=development lerna run build:watch --parallel",
    "start": "cross-env NODE_ENV=development run-p -r docs build:watch",
    "start-prod": "cross-env NODE_ENV=production run-p -r docs build:watch",
    "start-docker": "lerna run start-docker --stream",
    "clean": "lerna run clean",
    "bootstrap": "lerna bootstrap --hoist && node scripts/postinstall",
    "setup": "npm ci && npm run bootstrap && npm run build:packages && npm run build:css",
    "fresh": "npm run clean && lerna clean --yes && rimraf node_modules packages/bodiless-backend/.env examples/test-site/.env.development && npm run setup",
    "format": "prettier --write \"./packages/**/*.{js,jsx,ts,tsx}\"",
    "serve": "lerna run serve --scope @bodiless/test-site --stream",
    "scan": "ts-complexity --max-complexity 10 --pattern \"packages/**/src/**/*.ts(x)\"",
    "scan:target": "ts-complexity --max-complexity 1 --pattern",
    "cr:target": "cr",
    "lint": "eslint --cache --ext .js,.jsx,.ts,.tsx packages examples",
    "lint-target": "eslint --cache --ext .js,.jsx,.ts,.tsx --",
    "lint-change": "git diff --name-only --diff-filter=d master... | grep -e 'tsx$' -e 'jsx$' -e 'js$' -e 'ts$' | xargs -L 1 npm run lint-target",
    "fix": " eslint --fix  --cache --ext .js,.jsx,.ts,.tsx packages examples",
    "fix-target": "eslint --fix  --cache --ext .js,.jsx,.ts,.tsx -- ",
    "test": "jest",
    "test-watch": "jest --watch",
    "lerna": "lerna",
    "docs": "lerna run docs --stream",
    "lerna:version": "lerna version",
    "publish:all": "npm run build && lerna publish --registry $NPM_REGISTRY",
    "publish:from-package": "npm run build && lerna publish from-package --registry $NPM_REGISTRY",
    "migrate": "DEBUG=migration_tool node packages/bodiless-migration-tool/bin/run",
    "migrate:postbuild": "node packages/bodiless-migration-tool/bin/run postbuild examples/test-site/public",
    "copyright": "node ./scripts/add-copyright"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint"
    }
  },
  "dependencies": {
    "@types/html2canvas": "0.0.35",
    "complexity-report": "^2.0.0-alpha",
    "debug": "^4.1.1",
    "html2canvas": "^1.0.0-rc.3",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "symlink-dir": "^3.0.3",
    "ts-complexity": "0.0.3",
    "typescript": "^3.4.5"
  },
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.2.1",
    "@babel/plugin-proposal-decorators": "^7.4.0",
    "@babel/runtime": "^7.4.2",
    "@types/enzyme": "^3.9.1",
    "@types/jest": "^23.3.13",
    "@types/lodash": "^4.14.123",
    "@types/rc-tooltip": "^3.7.1",
    "@types/react-dom": "^16.8.3",
    "@types/slate": "^0.44.0",
    "@types/slate-react": "^0.21.1",
    "@types/uuid": "^3.4.4",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.5.0",
    "cross-env": "^5.2.0",
    "docsify-cli": "^4.3.0",
    "dotenv": "^8.2.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-airbnb-typescript": "^4.0.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jest": "^22.1.2",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.11.0",
    "glob": "^7.1.6",
    "husky": "^3.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.5.0",
    "jest-haste-map": "^24.5.0",
    "jest-resolve": "^24.5.0",
    "jest-sonar-reporter": "^2.0.0",
    "lerna": "^3.11.0",
    "npm-run-all": "^4.1.5",
    "ora": "^4.0.2",
    "prettier": "^1.17.0",
    "react-test-renderer": "16.8.4",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.0",
    "tslint": "^5.12.1",
    "tslint-config-airbnb": "^5.11.1",
    "typescript": "^3.4.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/johnsonandjohnson/bodiless-js.git"
  },
  "engines": {
    "node": ">=10.5"
  }
}
