    "nuka-carousel": "^4.4.7",
    "ora": "^4.0.2",
    "rc-tooltip": "^3.7.3",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-helmet": "^5.2.0",
    "react-markdown": "^4.3.1",
    "react-mde": "^10.0.3",
    "tailwindcss": "^1.0"
