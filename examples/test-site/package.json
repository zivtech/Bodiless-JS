  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/johnsonandjohnson/bodiless-js.git"
  },
  "scripts": {
    "build": "npm-run-all build:env-vars build:lib build:scripts build:doc psh-validate build:search",
    "build:watch": "run-p -r dev-frontend dev-backend",
    "build:lib": "bl-clear-logs && gatsby build && bl-validate-logs",
    "build:doc": "bl-docs-build",
    "build:scripts": "tsc -p ./tsconfig.json",
    "build:env-vars": "generate-env-vars",
    "build:search": "npm run search-index",
    "check": "tsc -p ./tsconfig.check.json",
    "clean": "rimraf ./public/* ./.cache",
    "dev-backend": "node ./node_modules/@bodiless/backend/src/server.js",
    "dev-frontend": "gatsby develop -p 8005",
    "serve": "gatsby serve",
    "prebuild:watch": "run-s build:env-vars build:doc",
    "start": "npm run build:watch",
    "prestart": "run-s build:env-vars build:doc",
    "docs:build": "bl-docs-build",
    "docs:serve": "docsify serve ./doc",
    "docs": "run-s build:doc docs:serve",
    "psh-update": "node ./lib/scripts/bodiless-psh-update",
    "psh-validate": "node ./lib/scripts/bodiless-psh-validate",
    "search-index": "create-search-index"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.4.0",
    "copyfiles": "^2.1.1",
    "gatsby-plugin-compile-es6-packages": "^1.1.0",
    "npm-run-all": "^4.1.5",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.9.7"
  }
}
