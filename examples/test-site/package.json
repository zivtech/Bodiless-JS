    "test": "__tests__"
  },
  "dependencies": {
    "@babel/core": "^7.4.3",
    "@bodiless/accessibility": "^0.0.39",
    "@bodiless/backend": "^0.0.39",
    "@bodiless/bv": "^0.0.39",
    "@bodiless/components": "^0.0.39",
    "@bodiless/core": "^0.0.39",
    "@bodiless/core-ui": "^0.0.39",
    "@bodiless/documentation": "^0.0.39",
    "@bodiless/fclasses": "^0.0.39",
    "@bodiless/gatsby-plugin-ssi": "^0.0.39",
    "@bodiless/gatsby-theme-bodiless": "^0.0.39",
    "@bodiless/layouts": "^0.0.39",
    "@bodiless/layouts-ui": "^0.0.39",
    "@bodiless/migration-tool": "^0.0.39",
    "@bodiless/organisms": "^0.0.39",
    "@bodiless/psh": "^0.0.39",
    "@bodiless/richtext": "^0.0.39",
    "@bodiless/richtext-ui": "^0.0.39",
    "@bodiless/ui": "^0.0.39",
    "docsify-cli": "^4.3.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "gatsby": "^2.19.17",
    "gatsby-plugin-canonical-urls": "^2.1.18",
    "gatsby-plugin-sitemap": "^2.2.24",
    "lodash": "^4.17.11",
    "mobx-react-lite": "^1.3.1",
    "nuka-carousel": "^4.4.7",
    "ora": "^4.0.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-helmet": "^5.2.0",
    "tailwindcss": "^1.0"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/johnsonandjohnson/bodiless-js.git"
  },
  "scripts": {
    "build": "npm-run-all build:env-vars build:css build:lib build:scripts build:doc psh-validate",
    "build:watch": "run-p -r dev-frontend dev-backend",
    "build:lib": "gatsby build",
    "build:css": "node scripts/build-css.js",
    "build:doc": "bl-docs-build",
    "build:scripts": "tsc -p ./tsconfig.json",
    "build:env-vars": "generate-env-vars",
    "check": "tsc -p ./tsconfig.check.json",
    "clean": "rimraf ./public/* ./.cache",
    "dev-backend": "node ./node_modules/@bodiless/backend/src/server.js",
    "dev-frontend": "gatsby develop -p 8005",
    "serve": "gatsby serve",
    "prebuild:watch": "run-s build:env-vars build:css build:doc",
    "start": "npm run build:watch",
    "prestart": "run-s build:env-vars build:doc",
    "docs:build": "bl-docs-build",
    "docs:serve": "docsify serve ./doc",
    "docs": "run-s build:doc docs:serve",
    "psh-update": "node ./lib/scripts/bodiless-psh-update",
    "psh-validate": "node ./lib/scripts/bodiless-psh-validate",
    "prepare": "npm run build:css"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.4.0",
    "copyfiles": "^2.1.1",
    "gatsby-plugin-compile-es6-packages": "^1.1.0",
    "npm-run-all": "^4.1.5",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.4.5"
  }
}
