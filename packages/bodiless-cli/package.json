{
  "name": "@bodiless/cli",
  "version": "0.0.62",
  "description": "A cli tool for Bodiless-JS",
  "author": "Chris Oden <coden@its.jnj.com>",
  "homepage": "",
  "license": "Apache-2.0",
  "main": "./lib/index.js",
  "bin": {
    "bodiless": "./bin/bodiless"
  },
  "directories": {
    "lib": "lib",
    "test": "__tests__",
    "bin": "bin"
  },
  "files": [
    "/bin",
    "/lib",
    "/bodiless.docs.json"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/johnsonandjohnson/bodiless-js.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc -p ./tsconfig.json ",
    "build:watch": "npm run build -- --watch",
    "postpack": "rimraf oclif.manifest.json",
    "prepack": "rimraf lib && npm run build && oclif-dev manifest && oclif-dev readme",
    "test": "echo \"Error: run tests from root\" && exit 1"
  },
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "@types/fs-extra": "^8.0.0",
    "@types/lodash": "^4.14.158",
    "@types/lodash.flow": "^3.5.6",
    "fs-extra": "^8.1.0",
    "globby": "^10",
    "lodash": "^4.17.19"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@types/node": "^10",
    "nodemon": "^1.19.1"
  },
  "oclif": {
    "commands": "./lib/commands",
    "repositoryPrefix": "https://github.com/johnsonandjohnson/Bodiless-JS/tree/master/packages/bodiless-cli/src",
    "bin": "bodiless",
    "plugins": [
      "@oclif/plugin-help"
    ]
  }
}
