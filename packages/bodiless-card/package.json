{
  "name": "@bodiless/card",
  "version": "0.2.2",
  "description": "Basic components to build out sites.",
  "author": "Guilherme Zeni <gzeni@its.jnj.com>",
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "/lib",
    "/bodiless.docs.json",
    "/doc",
    "tailwind.config.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/johnsonandjohnson/bodiless-js.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run copy && tsc -p ./tsconfig.json && npm run build:api-doc",
    "build:watch": "npm run build -- --watch",
    "build:api-doc": "typedoc --out doc/api --mode file",
    "lint": "tslint ./src/**/*.ts?x",
    "clean": "rimraf \"lib/*\" && rimraf tsconfig.tsbuildinfo && rimraf \"doc/api\"",
    "copy": "copyfiles -u 1 \"./src/**/*.{css,png}\" \"./lib/\""
  },
  "dependencies": {
    "@bodiless/core": "^0.2.2",
    "@bodiless/fclasses": "^0.2.2",
    "lodash": "^4.17.19",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/uuid": "^3.4.4"
  },
  "peerDependencies": {
    "react": "^16.8.4"
  }
}
